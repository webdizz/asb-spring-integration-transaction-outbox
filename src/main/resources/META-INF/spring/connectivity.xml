<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
">

    <bean id="connectionFactory"
          lazy-init="false"
          class="org.apache.qpid.jms.JmsConnectionFactory">
        <property name="remoteURI" value="amqp://0.0.0.0:5672"/>
<!--        <property name="username" value="sfs-asb-user"/>-->
<!--        <property name="password" value="LbQYpYI6hO855VCmeFrL7FY8whYpDkP8fdPrlZ7yryA="/>-->
<!--        <property name="remoteURI" value="amqps://sfs-asb-standard.servicebus.windows.net"/>-->
        <property name="clientIDPrefix" value="SFS-"/>
    </bean>

    <bean id="inboundQueue" class="org.apache.qpid.jms.JmsQueue">
        <constructor-arg>
            <value>orders</value>
        </constructor-arg>
    </bean>

</beans>